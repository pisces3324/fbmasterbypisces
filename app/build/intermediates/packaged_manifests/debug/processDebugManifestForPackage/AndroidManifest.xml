<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.facebook"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="36" />

    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="com.example.facebook.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.facebook.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/facebook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/facebook"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Facebook" >
        <activity
            android:name="com.example.facebook.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.facebook.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.ResetPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.HomeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.NotificationsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.FriendsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.VideoActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.MenuActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.SearchActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.SettingsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.SavedPostsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.PrivacyActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.MessengersActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.ProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.HelpActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.CreateStoryActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.CreatePostActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.CreateNoteActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.CreateFilmActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.StoryActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.example.facebook.GeminiActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.LiveStreamActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.CommentActivity"
            android:exported="false" />
        <activity
            android:name="com.example.facebook.StoryDetailActivity"
            android:exported="false" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.facebook.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>